{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "allowJs": true,
    "isolatedModules": true,
    "types": ["node"]
  }
}
